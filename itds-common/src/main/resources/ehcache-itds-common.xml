  <cache name="sysprops"
    maxEntriesLocalHeap="100"
    maxEntriesLocalDisk="100"
    eternal="false"
    diskSpoolBufferSizeMB="20"
    timeToIdleSeconds="30" timeToLiveSeconds="60"
    memoryStoreEvictionPolicy="LFU"
    transactionalMode="off">
    <persistence strategy="localTempSwap"/>
  </cache>
